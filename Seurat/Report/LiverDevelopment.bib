@incollection{zaret_chapter_2016,
	series = {Essays on {Developmental} {Biology}, {Part} {B}},
	title = {Chapter {Thirty}-{Eight} - {From} {Endoderm} to {Liver} {Bud}: {Paradigms} of {Cell} {Type} {Specification} and {Tissue} {Morphogenesis}},
	volume = {117},
	shorttitle = {Chapter {Thirty}-{Eight} - {From} {Endoderm} to {Liver} {Bud}},
	url = {http://www.sciencedirect.com/science/article/pii/S0070215315002227},
	abstract = {The early specification, rapid growth and morphogenesis, and conserved functions of the embryonic liver across diverse model organisms have made the system an experimentally facile paradigm for understanding basic regulatory mechanisms that govern cell differentiation and organogenesis. This essay highlights concepts that have emerged from studies of the discrete steps of foregut endoderm development into the liver bud, as well as from modeling the steps via embryonic stem cell differentiation. Such concepts include understanding the chromatin basis for the competence of progenitor cells to develop into specific lineages; the importance of combinatorial signaling from different sources to induce cell fates; the impact of inductive signaling on preexisting chromatin states; the ability of separately specified domains of cells to merge into a common tissue; and the marked cell biological dynamics, including interactions with the developing vasculature, which establish the initial morphogenesis and patterning of a tissue. The principles gleaned from these studies, focusing on the 2 days it takes for the endoderm to develop into a liver bud, should be instructive for many other organogenic systems and for manipulating tissues in regenerative contexts for biomedical purposes.},
	urldate = {2019-07-25},
	booktitle = {Current {Topics} in {Developmental} {Biology}},
	publisher = {Academic Press},
	author = {Zaret, Kenneth S.},
	editor = {Wassarman, Paul M.},
	month = jan,
	year = {2016},
	doi = {10.1016/bs.ctdb.2015.12.015},
	keywords = {Chromatin, Hepatocyte, Induction, Liver, Pioneer factor, Signaling},
	pages = {647--669},
	file = {ScienceDirect Full Text PDF:/run/media/carno/Data/Grocery/Papers/storage/I8QKHUMK/Chapter Thirty-Eight - From Endoderm to Liver Bud.pdf:application/pdf}
}

@article{gao_fetal_2018,
