	pages = {885--892},
	file = {Fetal liver an ideal niche for hematopoietic stem.pdf:/run/media/carno/Data/Grocery/Papers/storage/925BENHI/Fetal liver an ideal niche for hematopoietic stem.pdf:application/pdf}
}

@article{han_mapping_2018,
	title = {Mapping the {Mouse} {Cell} {Atlas} by {Microwell}-{Seq}},
