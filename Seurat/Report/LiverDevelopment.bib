	month = feb,
	year = {2018},
	keywords = {cell type classification, cellular heterogeneity, cross-tissue cellular network, mammalian cell map, Microwell-seq, mouse cell atlas, scMCA analysis, single cell RNA-seq, single-cell analysis, transcriptome analysis},
	pages = {1091--1107.e17},
	file = {ScienceDirect Full Text PDF:/run/media/carno/Data/Grocery/Papers/storage/GZGIPBV3/Mapping the Mouse Cell Atlas by Microwell-Seq.pdf:application/pdf}
